            <!--CORE JAVASCRIPT-->
            <script>
                function MaskMoney(obj, evt, deci_point) {
                    var parts = obj.value.split('.');
                    var tabkey = evt.keyCode;
                    switch (evt.keyCode) {
                        // Digits 0..9             
                        case 9:
                        case 96:
                        case 97:
                        case 98:
                        case 99:
                        case 100:
                        case 101:
                        case 102:
                        case 103:
                        case 104:
                        case 105:
                        case 48:
                        case 49:
                        case 50:
                        case 51:
                        case 52:
                        case 53:
                        case 54:
                        case 55:
                        case 56:
                        case 57:
                            // Accept only one 14 integer digits and 2 decimal places
                            if (tabkey != 9) {
                                if (parts.length < 2)
                                    return (parts[0].length < 14)
                                else
                                    return (parts[1].length < deci_point);
                                break;
                            }
                            // Decimal point
                        case 190:
                            // Accept only one single point
                            if (tabkey != 9) {
                                return (parts.length < deci_point);
                                break;
                            }
                            // Backspace (music), Delete (46)
                        case 8:
                        case 46:
                            return (parts.length > 0);
                            break;

                            // Left (37), Right (39) arrow keys             
                            // Add here cases for any other key code you want to accept             
                        case 37:
                        case 39:
                            return true;
                            break;

                        case 110:
                            // Accept only one single point
                            return (parts.length < 2);
                            break;
                            // Anything else is invalid             
                        default:
                            return false;
                    }
                }
            </script>
            <!-- BEGIN DISABLE ENTER KEY -->
            <script>
                function ignoreEnter(event) {

                    if (event.keyCode == 13) {
                        return false;
                    }
                }
            </script>
